{
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 2,
  "semi": false,
  "useTabs": false,
  "bracketSameLine": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "svelteBracketNewLine": false,
  "htmlWhitespaceSensitivity": "ignore",
  "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
